<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC -//Apple Computer//DTD PLIST 1.0//EN
http://www.apple.com/DTDs/PropertyList-1.0.dtd>
<plist version="1.0">
<dict>
    <key>Label</key>
		<string>com.can.restic_backup</string>
    
    <key>Program</key>
		<string>/Users/can/.restic/backup_wrapper</string>

    <key>RunAtLoad</key>
    <true/>

    <key>StartInterval</key>
    <integer>300</integer>
    
    <key>WorkingDirectory</key>
		<string>/Users/can/</string>
    
    <key>StandardOutPath</key>
		<string>/Users/can/Library/Logs/com.can.restic_backup_out.log</string>

    <key>StandardErrorPath</key>
		<string>/Users/can/Library/Logs/com.can.restic_backup_error.log</string>
</dict>
</plist>
